{
  "name": "@kkt/template-react-component-tsx",
  "version": "5.6.1",
  "description": "React Component Example for TypeScript.",
  "private": true,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "prepare": "npm run build",
    "released": "npm run build && npm run doc",
    "doc": "ENTRYDIR=website kkt build",
    "doc:dev": "ENTRYDIR=website kkt start",
    "build": "npm run ts:build && npm run types:esm && npm run types:cjs && npm run css:build",
    "types:build": "tsbb types --sourceRoot src --target ESNEXT",
    "types:watch": "npm run types:esm -- --watch & npm run types:cjs -- --watch",
    "types:esm": "npm run types:build -- --outDir ../lib/esm",
    "types:cjs": "npm run types:build -- --outDir ../lib/cjs",
    "css:build": "compile-less -d src -o lib/esm",
    "ts:watch": "tsbb watch --env-name esm:dev --env-name cjs --target react",
    "ts:build": "tsbb build --target react"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kktjs/kkt.git"
  },
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@kkt/loader-less": "5.6.1",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "compile-less-cli": "^1.1.5",
    "kkt": "5.6.1",
    "raw-loader": "^3.1.0",
    "tsbb": "^1.3.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
