{
  "name": "@template/electron",
  "version": "7.1.3",
  "description": "Electron Example.",
  "main": "lib/main.js",
  "private": true,
  "scripts": {
    "start": "kkt start --no-open-browser",
    "build": "GENERATE_SOURCEMAP=false kkt build",
    "watch": "tsbb watch --no-esm --no-source-maps --entry app/main.ts",
    "build:main": "tsbb build --no-esm --no-source-maps --entry app/main.ts",
    "app": "NODE_ENV=development ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "app:build": "electron ."
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@kkt/less-modules": "7.1.2",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "electron": "~18.0.3",
    "kkt": "7.1.7",
    "tsbb": "~3.7.0"
  },
  "dependencies": {
    "react": "~18.1.0",
    "react-dom": "~18.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
